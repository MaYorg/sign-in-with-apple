{
  "name": "kasparsd/sign-in-with-apple",
  "description": "A WordPress plugin for adding the Signin with Apple",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Kaspars Dambis",
      "email": "hi@kaspars.net"
    }
  ],
  "require": {
    "php": ">=5.6",
    "lcobucci/jwt": "^3.3"
  },
  "require-dev": {
    "wp-coding-standards/wpcs": "^2.1",
    "wpsh/local": "^0.2.2"
  },
  "autoload": {
    "psr-4": {
      "WPSH\\SignInWithApplePlugin\\": "src/"
    }
  },
  "scripts": {
    "lint": [
      "phpcs ."
    ]
  },
  "config": {
    "autoloader-suffix": "WPSHSignInWithApplePlugin",
    "platform": {
      "php": "5.6"
    }
  }
}
